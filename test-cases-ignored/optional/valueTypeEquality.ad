public main(console: mut system.console.Console) -> void
{
	let v: int = 42;
	let n: int? = none;
	var r: bool;

	// Equal
	r = v == n;
	console.WriteLine("v == n: " + r);
	r = n == v;
	console.WriteLine("n == v: " + r);
	r = n == n;
	console.WriteLine("n == n: " + r);
	r = 42 == none;
	console.WriteLine("42 == none: " + r);
	r = none == 42;
	console.WriteLine("none == 42: " + r);
	r = none == none;
	console.WriteLine("none == none: " + r);

	// Not Equal
	r = v =/= n;
	console.WriteLine("v =/= n: " + r);
	r = n =/= v;
	console.WriteLine("n =/= v: " + r);
	r = n =/= n;
	console.WriteLine("n =/= n: " + r);
	r = 42 =/= none;
	console.WriteLine("42 =/= none: " + r);
	r = none =/= 42;
	console.WriteLine("none =/= 42: " + r);
	r = none =/= none;
	console.WriteLine("none =/= none: " + r);
}

/// The root binding scope that accounts for package names
public /*mut*/ class Package_Binding_Scope
{
	private let package_declarations: mut System.Collections.List<Package_Binding_Declaration>;

	public new()
	{
		self.package_declarations = new System.Collections.List<Package_Binding_Declaration>();
		self.package_declarations.add(new Package_Binding_Declaration(self, new Package_Name("$any")));
	}
}

/// Names a namespace without reference to which package it is in.
///
/// The global namespace is represented as a namespace name with no segments
public class Namespace_Name
{
	public let segments: System.Collections.List<string>;

	public new(segments: System.Collections.List<string>)
	{
		self.segments = segments;
	}
}

// TODO make this an associated function
public global_namespace() -> Namespace_Name
{
	return new Namespace_Name(new System.Collections.List<string>());
}
